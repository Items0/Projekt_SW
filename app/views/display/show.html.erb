<%= button_to "Back", root_path, :method => :get, class: "btn" %>


<h1><%= @user.name %> </h1>

<% @types.each do |x| %>
	<p> <%= x.capitalize %> </p>
	<% @myElements = @measurements.where(kind: x) %>

	<%= line_chart @myElements.pluck(:created_at, :value), xtitle: "Args", ytitle: "Values" %>


<% end %>

